<ul class="rest-links unstyled">

    {% if site %}
        <li><a href="{{site}}" aria-label="website">
            {% include "_includes/icons/globe.svg" %}
        </a></li>
    {% endif %}

    {% if facebook %}
        <li><a href="{{facebook}}" aria-label="facebook">
            {% include "_includes/icons/facebook.svg" %}
        </a></li>
    {% endif %}

    {% if twitter %}
        <li><a href="{{twitter}}" aria-label="twitter">
            {% include "_includes/icons/twitter.svg" %}
        </a></li>
    {% endif %}

    {% if instagram %}
        <li><a href="{{instagram}}" aria-label="instagram">
            {% include "_includes/icons/instagram.svg" %}
        </a></li>
    {% endif %}

</ul>


{# All Content Injected Here #}
{{ content | safe }}
